{
    "manifest_version": 3,
    "name": "Inclusive Canvas",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["https://uofa.instructure.com/*"],
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "src/background.ts",
      "type": "module"
    },
    "content_scripts": [
      {
        "matches": ["https://uofa.instructure.com/*"],
        "js": ["src/content.ts"]
      }
    ]
  }